<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                   xmlns:app="http://schemas.android.com/apk/res-auto"
                                                   xmlns:tools="http://schemas.android.com/tools"
                                                   android:layout_width="match_parent"
                                                   android:layout_height="match_parent"
                                                   tools:context=".ChecklistFragment" android:id="@+id/frameLayout2">
    <CheckBox
            android:layout_width="32dp"
            android:layout_height="19dp" android:id="@+id/checkBox6"
            tools:layout_editor_absoluteY="134dp" app:layout_constraintStart_toEndOf="@+id/checkBox5"
            android:layout_marginLeft="16dp" android:layout_marginStart="16dp"/>
    <CheckBox
            android:layout_width="32dp"
            android:layout_height="19dp" android:id="@+id/checkBox5"
            tools:layout_editor_absoluteY="134dp" app:layout_constraintStart_toEndOf="@+id/checkBox4"
            android:layout_marginLeft="16dp" android:layout_marginStart="16dp"/>
    <CheckBox
            android:layout_width="32dp"
            android:layout_height="19dp" android:id="@+id/checkBox4"
            tools:layout_editor_absoluteY="134dp" app:layout_constraintStart_toStartOf="@+id/textView6"
            android:layout_marginLeft="251dp" android:layout_marginStart="251dp"/>
    <CheckBox
            android:layout_width="32dp"
            android:layout_height="19dp" android:id="@+id/checkBox2"
            tools:layout_editor_absoluteY="90dp" app:layout_constraintStart_toEndOf="@+id/checkBox"
            android:layout_marginLeft="16dp" android:layout_marginStart="16dp"/>
    <CheckBox
            android:layout_width="32dp"
            android:layout_height="19dp" android:id="@+id/checkBox3"
            tools:layout_editor_absoluteY="90dp" app:layout_constraintStart_toEndOf="@+id/checkBox2"
            android:layout_marginLeft="16dp" android:layout_marginStart="16dp"/>
    <TextView
            android:text="Checklist"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:id="@+id/CheckListhead"
            android:textSize="34sp"
            android:background="#FFF387"
            android:textColor="#000000"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
    />
    <CheckBox
            android:layout_width="32dp"
            android:layout_height="19dp" android:id="@+id/checkBox"
            tools:layout_editor_absoluteY="90dp" tools:layout_editor_absoluteX="267dp"/>
    <TextView
            android:text="N/A"
            android:layout_width="32dp"
            android:layout_height="wrap_content" android:id="@+id/Irrelevant"
            tools:layout_editor_absoluteX="363dp" app:layout_constraintBottom_toTopOf="@+id/checkBox3"
            android:layout_marginBottom="8dp"/>
    <TextView
            android:text="Nej"
            android:layout_width="32dp"
            android:layout_height="wrap_content" android:id="@+id/nej"
            tools:layout_editor_absoluteX="315dp" app:layout_constraintBottom_toTopOf="@+id/checkBox2"
            android:layout_marginBottom="8dp"/>
    <TextView
            android:text="Ja"
            android:layout_width="32dp"
            android:layout_height="wrap_content" android:id="@+id/ja"
            tools:layout_editor_absoluteX="267dp" app:layout_constraintBottom_toTopOf="@+id/checkBox"
            android:layout_marginBottom="8dp"/>
    <TextView
            android:text="Er der taget hensyn til ydre påvirkninger og anvendt korrekt kapslingsklasse?"
            android:layout_width="0dp"
            android:layout_height="wrap_content" android:id="@+id/firstLine"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="16dp" android:layout_marginStart="16dp"
            app:layout_constraintTop_toBottomOf="@+id/CheckListhead" android:layout_marginTop="40dp"
            app:layout_constraintEnd_toStartOf="@+id/checkBox"/>
    <TextView
            android:text="Er der brandtætnet ved gennemføringer?"
            android:layout_width="0dp"
            android:layout_height="wrap_content" android:id="@+id/textView6"
            app:layout_constraintStart_toStartOf="@+id/firstLine" app:layout_constraintTop_toBottomOf="@+id/firstLine"
            android:layout_marginTop="8dp"
            app:layout_constraintEnd_toStartOf="@+id/checkBox"/>
</androidx.constraintlayout.widget.ConstraintLayout>